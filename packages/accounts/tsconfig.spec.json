{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@ports": ["./src/ports/index.ts"],
      "@types": ["./src/types/index.ts"],
      "@services": ["./src/services/index.ts"],
      "@constants": ["./src/constants.ts"],
      "@core": ["./src/core/index.ts"]
    },
    "outDir": "./out-tsc/vitest",
    "types": [
      "vitest/globals",
      "vitest/importMeta",
      "vite/client",
      "node",
      "vitest"
    ],
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "../../apps/deploy/vite.config.mts",
    "vitest.config.mts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.tsx",
    "src/**/*.test.js",
    "src/**/*.spec.js",
    "src/**/*.test.jsx",
    "src/**/*.spec.jsx",
    "src/**/*.d.ts"
, "src/services/query-handlers/list-user-accounts-query-handler.ts"  ],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    }
  ]
}
